{
	"manifest_version" : 2,
	"name" : "Chrome-Extension magic actions belt hider",
	"version" : "0.0.1",
	"content_scripts" : [
		{
			"matches" : [
                "https://youtube.com/*",
                "https://www.youtube.com/*"
             ],
             "js" : ["content.js"]
        }
     ],
     "background": {
        "scripts": ["background.js"]
      },
     "browser_action" :{

     },
     "permissions":[
        "tabs"
    ]
} 